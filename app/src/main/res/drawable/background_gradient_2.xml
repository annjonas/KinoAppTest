<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient android:startColor="#995623A2"
                android:centerColor="#41069E"
                android:endColor="#673AB7"
                android:type="radial"
                android:gradientRadius="110%p"
                android:centerX="0.2"
                android:centerY=""/>
        </shape>
    </item>
    <item tools:ignore="StateListReachable">
        <shape>
            <gradient android:startColor="#09355D"
                android:centerColor="@android:color/transparent"
                android:endColor="@android:color/transparent"
                android:type="radial"
                android:gradientRadius="100%p"
                android:centerX="0.5"
                android:centerY="0.5"/>
        </shape>
    </item>
    <item tools:ignore="StateListReachable">
        <shape>
            <gradient android:startColor="#042599"
                android:centerColor="@android:color/transparent"
                android:endColor="@android:color/transparent"
                android:type="radial"
                android:gradientRadius="100%p"
                android:centerX="0.5"
                android:centerY="0.5"/>
        </shape>
    </item>
</selector>